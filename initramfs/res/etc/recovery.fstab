#[mount]   [fs1]  [dev]                 [dev2]  [fs2]  [fsopts1]  [fsopts2]
/boot      bml    boot
/cache     ext4   /dev/block/stl11       NULL    rfs    NULL      rw,nosuid,nodev,vfat,llw,check=no,gid/uid/rwx,iocharset=utf8
/data      ext4   /dev/block/stl10       NULL    rfs    NULL      rw,nosuid,nodev,vfat,llw,check=no,gid/uid/rwx,iocharset=utf8
/recovery  bml    recovery
/sdcard    vfat   /dev/block/mmcblk0p1
/system    ext4   /dev/block/stl9        NULL    rfs    NULL      rw,vfat,llw,check=no,gid/uid/rwx,iocharset=utf8
